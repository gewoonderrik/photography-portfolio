{% extends 'base.twig' %}

{% block content %}
  <h1>
    {{ title }}
  </h1>

  {% if posts is not empty %}
    <div class="portfolio-wrap">
      {% for post in posts %}
        {% include 'partials/portfolio.twig' %}
      {% endfor %}
    </div>
  {% else %}
    <p>
      No results.
    </p>
  {% endif %}
{% endblock %}
